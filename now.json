{
  "version": 2,
  "name": "YAPP",
  "alias": ["podcast.bullinger.dev"],
  "build": {
    "env": {
      "ENGINE_API_KEY": "@engine_api_key"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(?!.*\\.(css|js|html|png|svg|jpg|gif|json)$)(.*)",
      "dest": "/"
    }
  ],
  "scope": "bullingermathis"
}
