{
  "description": "Yet Another Podcast Player",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env dev",
    "build": "npx webpack --env prod --config webpack.config.js",
    "build:dev": "npx webpack --env dev --config webpack.config.js",
    "now-build": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier:check": "prettier --check \"./*.{js,jsx,ts,tsx,html}\" \"./**/*.{js,jsx,ts,tsx,html}\"",
    "prettier:write": "prettier --write \"./*.{js,jsx,ts,tsx,html}\" \"./**/*.{js,jsx,ts,tsx,html}\"",
    "eslint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:write": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "format": "npm run eslint:write && npm run prettier:write"
  },
