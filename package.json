{
  "name": "yapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npx webpack-dev-server --env dev",
    "start:network": "echo \"\n\n\u001b[1mrunning at \u001b[32mhttp://$(ifconfig | grep \"inet \" | grep -Fv 127.0.0.1 | awk '{print $2}'):1234\u001b[0m\n\n\" && npm start -- --host='0.0.0.0'",
    "build": "npm run apollo:gen && npx webpack --env prod --config webpack.config.js",
    "build:dev": "npm run apollo:gen && npx webpack --env dev --config webpack.config.js",
    "now-build": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier:check": "prettier --check \"./*.{js,jsx,ts,tsx,html}\" \"./**/*.{js,jsx,ts,tsx,html}\"",
    "prettier:write": "prettier --write \"./*.{js,jsx,ts,tsx,html}\" \"./**/*.{js,jsx,ts,tsx,html}\"",
    "eslint:check": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "eslint:write": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "format": "npm run eslint:write && npm run prettier:write",
    "apollo:gen": "npx apollo client:codegen --target typescript --outputFlat src/gqlTypes",
    "mapsize": "npx source-map-explorer public/*.js*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathisBullinger/YAPP.git"
  },
  "author": "Mathis Bullinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MathisBullinger/YAPP/issues"
  },
  "homepage": "https://github.com/MathisBullinger/YAPP#readme",
