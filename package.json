{
  "description": "Yet Another Podcast Player",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html --no-autoinstall",
    "build": "rm -rf .cache/ dist/ && parcel build --no-source-maps src/index.html",
    "now-build": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:check": "prettier --check \"./*.{js,jsx,ts,tsx,html}\" \"./**/*.{js,jsx,ts,tsx,html}\"",
    "format:write": "prettier --write \"./*.{js,jsx,ts,tsx,html}\" \"./**/*.{js,jsx,ts,tsx,html}\"",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:write": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format-lint:check": "npm run lint:check && npm run format:check",
    "format-lint:write": "npm run lint:write && npm run format:write"
  },
  "alias": {
    "~": "src",
    "data": "./data",
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathisBullinger/YAPP.git"
  },
  "author": "Mathis Bullinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MathisBullinger/YAPP/issues"
  },
  "homepage": "https://github.com/MathisBullinger/YAPP#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "parcel-bundler": "*",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "preact": "^8.5.1",
    "preact-compat": "^3.19.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "styled-components": "^3.1.6"
  }
}
